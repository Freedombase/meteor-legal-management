    "publish": "npm prune --production && meteor publish && npm i",
    "all-contributors": "./node_modules/.bin/all-contributors",
    "all-contributors-generate": "./node_modules/.bin/all-contributors generate",
  },
  "author": "Jan Dvorak",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "all-contributors-cli": "^6.26.1"
